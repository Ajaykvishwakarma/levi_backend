<!DOCTYPE html>
<html lang="en">

<head>
    <link rel="stylesheet" href="/css/upi.css">
    <title>Payment Page</title>
</head>

<body>
    <div id="mainDiv">
        <div id="leftDiv">
            <img onclick="backToPaymentMethods()" id="leftArrow" src="https://api.payu.in/public/assets/images/back.svg"
                alt="leftArrow">
            <!-- <span>Back</span> -->
            <img id="leviLogo"
                src="data:image/jpg;base64,"
                alt="leviLogo">

            <p id="p1">Enter Any UPI ID</p>
            <p id="p2">Total Payable Amount â‚¹ <span id="totalAmt"></span></p>
            <p id="p3">Transaction Id: 6149457427143286</p>

        </div>
        <div id="rightDiv">
            <span>UPI OPTIONS</span>
            <p></p>
            <div class="payMethods">
                <img src="https://api.payu.in/public/assets/images/upi-logos/upi2/Gpay.svg" alt="gpay">
                <div onclick="gpay()">Google Pay </div>
                <img class="rightDropdown" src="https://api.payu.in/public/assets/images/accordian-arrw.svg"
                    alt="dropdownKey">
            </div>

            <div class="payMethods">
                <img src="https://api.payu.in/public/assets/images/upi-logos/upi2/PhonePe.svg" alt="phonePe">
                <div onclick="phonePe()">Phone Pe</div>
                <img class="rightDropdown" src="https://api.payu.in/public/assets/images/accordian-arrw.svg"
                    alt="dropdownKey">
            </div>

            <div class="payMethods">
                <img src="https://api.payu.in/public/assets/images/upi-logos/upi2/Bhim.svg" alt="bhim">
                <div onclick="bhim()">BHIM</div>
                <img class="rightDropdown" src="https://api.payu.in/public/assets/images/accordian-arrw.svg"
                    alt="dropdownKey">
            </div>

            <div class="payMethods">
                <img src="https://api.payu.in/public/assets/images/upi-logos/upi2/Paytm.svg" alt="paytm">
                <div onclick="paytm()">Paytm</div>
                <img class="rightDropdown" src="https://api.payu.in/public/assets/images/accordian-arrw.svg"
                    alt="dropdownKey">
            </div>

            <div class="payMethods">
                <img src="https://api.payu.in/public/assets/images/upi-logos/upi2/amazonpay.svg" alt="amazonPay">
                <div onclick="amazonPay()">Amazon Pay</div>
                <img class="rightDropdown" src="https://api.payu.in/public/assets/images/accordian-arrw.svg"
                    alt="dropdownKey">
            </div>

            <div class="payMethods">
                <img src="https://api.payu.in/public/assets/images/upi-logos/upi2/try.svg" alt="plusSign">
                <div onclick="anyUpi()">Any UPI ID</div>
                <img class="rightDropdown" src="https://api.payu.in/public/assets/images/accordian-arrw.svg"
                    alt="dropdownKey">
            </div>

        </div>
    </div>
</body>

</html>
<script>
    function backToPaymentMethods() {
        window.location.href = "/paymentMethods"
    }
    const amount = document.getElementById("totalAmt");
    amount.textContent = localStorage.getItem("amount");

</script>